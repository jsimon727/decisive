<div class='container-sm p-5' id='session-form'>
  <h2 class='pb-4'>LOGIN</h2>
  <div class="g-signin2" data-onsuccess="onSignIn"></div>
  <hr>
  <form action="/sessions" method="post">
    <div class="form-group">
      <label for="email">EMAIL</label>
      <input type="text" name="email" class="form-control" aria-describedby="userEmail" placeholder="">
    </div>
    <div class="form-group">
      <label for="password">PASSWORD</label>
      <input type="text" name="password" class="form-control" aria-describedby="userPassword" placeholder="">
    </div>
    <button type="submit" class="btn btn-primary float-right">Create A Poll</button>
  </form>
</div>
